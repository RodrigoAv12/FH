WA;AACA,WAAA,KAAA,CAAA,WAAA,EAAA,CAAA;AACA;AAbA;AApBA,CAAA","sourcesContent":["<template>\r\n  <v-container id=\"example\">\r\n    <v-row align=\"center\" justify=\"center\" v-for=\"n in c\" :key=\"n\">\r\n      <v-col cols=\"12\" md=\"12\">\r\n        <form>\r\n          <v-row>\r\n            <v-col class=\"d-flex\" cols=\"12\" md=\"4\">\r\n              <v-select\r\n                :items=\"cursos\"\r\n                v-model=\"cursos_tipo\"\r\n                label=\"Tipo\"\r\n              ></v-select>\r\n            </v-col>\r\n            <v-col cols=\"12\" md=\"8\">\r\n              <v-text-field\r\n                v-model=\"cursos_titulo\"\r\n                label=\"Título de Estudios Realizados\"\r\n                required\r\n              ></v-text-field>\r\n            </v-col>\r\n          </v-row>\r\n          <v-row>\r\n            <v-col cols=\"12\" md=\"4\">\r\n              <v-menu\r\n                ref=\"menu\"\r\n                v-model=\"menu\"\r\n                lang=\"spanish\"\r\n                :close-on-content-click=\"false\"\r\n                transition=\"scale-transition\"\r\n                offset-y\r\n                full-width\r\n                min-width=\"290px\"\r\n              >\r\n                <template v-slot:activator=\"{ on }\">\r\n                  <v-text-field\r\n                    v-model=\"cursos_fecha\"\r\n                    label=\"Fecha\"\r\n                    readonly\r\n                    v-on=\"on\"\r\n                  ></v-text-field>\r\n                </template>\r\n                <v-date-picker\r\n                  ref=\"picker\"\r\n                  v-model=\"cursos_fecha\"\r\n                  :max=\"new Date().toISOString().substr(0, 10)\"\r\n                  min=\"1950-01-01\"\r\n                  @change=\"save\"\r\n                ></v-date-picker>\r\n              </v-menu>\r\n            </v-col>\r\n            <v-col cols=\"12\" md=\"8\">\r\n              <v-text-field\r\n                v-model=\"cursos_medio\"\r\n                label=\"Centro de Estudios/Medio de Publicación\"\r\n                required\r\n                @input=\"$v.name.$touch()\"\r\n                @blur=\"$v.name.$touch()\"\r\n              ></v-text-field>\r\n            </v-col>\r\n          </v-row>\r\n        </form>\r\n      </v-col>\r\n    </v-row>\r\n    <!-------Botón para agregar------->\r\n    <v-row>\r\n      <v-col cols=\"12\" md=\"12\" justify-self=\"center\">\r\n        <v-btn class=\"ma-2\" color=\"teal\" dark @click=\"agregar()\">\r\n          <v-icon dark left>fas fa-plus</v-icon>Agregar\r\n        </v-btn>\r\n        <v-btn\r\n          v-show=\"c >= 2\"\r\n          class=\"ma-2\"\r\n          color=\"pink darken-1\"\r\n          dark\r\n          @click=\"eliminar()\"\r\n        >\r\n          <v-icon dark left>fas fa-minus</v-icon>Eliminar\r\n        </v-btn>\r\n      </v-col>\r\n    </v-row>\r\n    <v-row class=\"pa-2\" justify=\"center\">\r\n      <v-col class=\"d-flex pa-4\" cols=\"12\" sm=\"6\" md=\"12\">\r\n        <p style=\"font-family: 'Jomolhari', serif;\">\r\n          Lorem ipsum dolor sit amet consectetur adipisicing elit. Reiciendis\r\n          tempora alias aliquid quae beatae accusamus, sequi culpa labore ullam\r\n          praesentium rem. Laborum sit esse eveniet aperiam illum. Sapiente,\r\n          pariatur vel?\r\n        </p>\r\n      </v-col>\r\n    </v-row>\r\n    <v-row>\r\n      <v-btn\r\n        dark\r\n        @click=\"siguiente(false)\"\r\n        style=\"margin-right:10px\"\r\n        >Regresar</v-btn\r\n      >\r\n      <v-btn dark  @click=\"agregarCursos(), siguiente(true)\" style=\"margin-right:10px\"\r\n        >Siguiente</v-btn\r\n      >\r\n    </v-row>\r\n  </v-container>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  data() {\r\n    return {\r\n      cursos_titulo: \"\",\r\n      cursos_tipo: \"\",\r\n      cursos_fecha: \"\",\r\n      cursos_medio: \"\",\r\n      el: \"#example\",\r\n      c: 1,\r\n      cursos: [\r\n        \"Curso\",\r\n        \"Diplomado\",\r\n        \"Seminario\",\r\n        \"Certificación\",\r\n        \"Clínica\",\r\n        \"Congreso\",\r\n        \"Conferencia\"\r\n      ]\r\n    };\r\n  },\r\n  methods: {\r\n    agregarCursos() {\r\n      var cursos = {\r\n        cursos_titulo: this.cursos_titulo,\r\n        cursos_tipo: this.cursos_tipo,\r\n        cursos_fecha: this.cursos_fecha,\r\n        cursos_medio: this.cursos_medio\r\n      };\r\n      this.$store.dispatch(\"agregarCursos\", cursos);\r\n      \r\n    },\r\n    siguiente(x) {\r\n      this.$emit(\"siguiente\",x);\r\n    }\r\n  }\r\n};\r\n</script>\r\n"],"sourceRoot":"src/components/formularios"}]}